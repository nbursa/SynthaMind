# Ignore environment files (credentials, secrets)
.env
.env.local
.env.development
.env.production

# Ignore compiled Go binaries
*.exe
*.out
*.o
*.a

# Ignore dependency directories (Go modules & Python)
vendor/
bin/
obj/
venv/

# Python cache and logs
__pycache__/
*.pyc
*.pyo
*.log

# Ignore database and data files
*.db
*.sqlite
*.sqlite3
*.log
*.csv
*.json

# Ignore ChromaDB files
chromadb_data/

# Ignore ChromaDB storage, but keep configs if needed
chromadb_data/*
!chromadb_data/config.json

# Go build cache & temporary files
/tmp/
/build/

# Ignore macOS system files
.DS_Store

# Ignore VSCode settings
.vscode/

# Ignore logs
logs/
logs/*.log
tests/logs/
tests/logs/*.log
